{
  "name": "ng2-tour",
  "version": "0.0.3",
  "description": "Product tour library build in Angular (2+)",
  "main": "build/src/app/lib/tour.module.js",
  "scripts": {
    "cleanup": "rimraf build dist src/app/**/*.ngfactory.ts src/app/**/*.shim.ts",
    "explorer": "source-map-explorer ./dist/bootstrap.bundle.js ./dist/bootstrap.bundle.js.map",
    "explorer-aot": "source-map-explorer ./dist/bootstrap.aot.bundle.js ./dist/bootstrap.aot.bundle.js.map",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "serve": "http-server ./dist -a 0.0.0.0 -p 9000",
    "start": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server\" \"node ./bin/ngc-watch.js\"",
    "start-plugin": "npm run cleanup && webpack-dev-server --config webpack.aot.config.js",
    "start-prod": "npm run ngc && npm run webpack-prod && npm run serve",
    "build": "npm run cleanup && npm run ngc && npm run webpack-prod",
    "deploy": "gh-pages -d dist",
    "prepublish": "npm run cleanup && npm run ngc",
    "webpack": "webpack --config webpack.config.js",
    "webpack-aot": "webpack --config webpack.aot.config.js",
    "webpack-prod": "webpack --config webpack.prod.config.js"
  },
  "keywords": [
    "angular",
    "angular2",
    "tour"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaacplmann/ng2-tour.git"
  },
  "author": {
    "name": "isaacplmann",
    "email": "isaacplmann@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/router": "3.2.4",
    "@ng-bootstrap/ng-bootstrap": "file:./ng-bootstrap-ng-bootstrap-1.0.0-alpha.15.tgz",
    "@types/core-js": "^0.9.35",
    "angular2-hotkeys": "^0.5.0",
    "core-js": "2.4.1",
    "rxjs": "^5.0.0-rc.4",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.4",
    "@angular/platform-server": "2.2.4",
    "@ngtools/webpack": "1.1.7",
    "@types/node": "6.0.48",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "chokidar": "1.6.1",
    "codelyzer": "^2.0.0-beta.3",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "gh-pages": "0.12.0",
    "http-server": "0.9.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.0",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "tslint": "^4.0.2",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "2.0.10",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  }
}
